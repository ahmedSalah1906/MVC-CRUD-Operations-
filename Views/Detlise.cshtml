@{
    ViewData["Title"] = "Details";
}
@model Instructor

<h1>Details</h1>
<div class="container">
    <img src="@Model.image_url" width="200" height="200" alt="Instructor Image" />
    <div>
        <strong>Name:</strong> @Model.Name
    </div>
    <div>
        <strong>Course:</strong> @Model.Course.Name
    </div>
    <div>
        <strong>Address:</strong> @Model.address
    </div>
    <button class="btn btn-success" onclick="location.href=/Instructor/Index">Back</button>
</div>
