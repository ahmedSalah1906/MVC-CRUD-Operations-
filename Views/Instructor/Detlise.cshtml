@{
    ViewData["Title"] = "Details";
}
@model Instructor

<h1>Details</h1>
<div class="container">
    <img src="/image/@Model.image_url" width="200" height="200" alt="Instructor Image" />
    <div>
        <p> <strong>Name:</strong> @Model.Name</p>
    </div>
    <div>
        <p><strong>Course:</strong> @Model.CourseID</p>
    </div>
    <div>
        <p><strong>Address:</strong> @Model.address</p>
    </div>
    <button class="btn btn-success" onclick="location.href='/Instructor/Index'">Back</button>
</div>
